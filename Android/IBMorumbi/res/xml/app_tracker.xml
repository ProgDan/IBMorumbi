<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
tools:ignore="TypographyDashes">

    <!-- The apps Analytics Tracking Id -->
    <string name="ga_trackingId">UA-50297107-1</string>
    
    <!-- Percentage of events to include in reports -->
    <string name="ga_sampleFrequency">100.0</string>
    
    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>
    
    <!-- catch and report uncaught exceptions from the app -->
    <bool name="ga_reportUncaughtExceptions">true</bool>
    
    <!-- How long a session exists before giving up -->
    <integer name="ga_sessionTimeout">-1</integer>
    
    <!-- The screen names that will appear in reports -->
    <screenName name="com.progdan.ibmorumbi.AudioActivity">MessageAudioPlayer Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.FragmentAbout">About Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.FragmentMap">Map Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.FragmentMessage">Message Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.FragmentNewsletter">Newsletter Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.FragmentWebsite">WebView Screen</screenName>
    <screenName name="com.progdan.ibmorumbi.VideoActivity">MessageVideoPlayer Screen</screenName>
</resources>
